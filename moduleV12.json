{
  "id": "pf2e-md-exporter",
  "title": "PF2E Exporter",
  "description": "Exports one or more journals as a ZIP file containing markdown files in a folder tree. This is specifically tuned for working with PF2E formatted data.",
  "version": "0.9.3-v12-beta.1",
  "library": "false",
  "compatibility": {
    "minimum": 12,
    "maximum": 12,
    "verified": "12.325.0"
  },
  "authors": [
		{
			"name": "MostTornBrain",
			"url": "https://github.com/MostTornBrain"
		}
  ],
  "relationships": {
    "requires": [
			{
				"id": "lib-wrapper",
				"manifest": "https://github.com/ruipin/fvtt-lib-wrapper/releases/latest/download/module.json"
			},
      {
				"id": "more-handlebars-helpers",
				"manifest": "https://github.com/kgar/foundry-vtt-more-handlebars-helpers/releases/latest/download/module.json"
			}

    ]
  },
  "esmodules": [
    "scripts/module-settings.js",
    "scripts/export-markdown.js",
    "scripts/lib/turndown.js",
    "scripts/lib/turndown-plugin-gfm.js"
  ],
  "scripts": [
    "scripts/lib/jszip.min.js"
  ],
  "styles": [
    "styles/module.css"
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "languages/en.json"
    }
  ],
  "url": "https://github.com/MostTornBrain/pf2e-md-exporter/tree/feature/v12-support",
  "manifest": "https://raw.githubusercontent.com/MostTornBrain/pf2e-md-exporter/feature/v12-support/moduleV12.json",
  "download": "https://github.com/MostTornBrain/pf2e-md-exporter/releases/download/v0.9.3-v12-beta.1/module.zip",
  "license": "LICENSE",
  "readme": "README.md"
}
